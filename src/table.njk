{% extends '_templates/base.njk' %}

{% block page_title %}
  Winners Table
{% endblock %}

{% block page_description %}
  A page with a searchable table for winners.
{% endblock %}

{% block content %}
<article class="container">
  <h1>Winners Table</h1>
  <div class="row justify-content-center">
    <div class="table" style="min-height:926px">
      <script type="text/javascript" defer src="https://datawrapper.dwcdn.net/ty2Gy/embed.js?v=1" charset="utf-8"></script>
      <noscript><img src="https://datawrapper.dwcdn.net/ty2Gy/full.png" alt="Searchable table of F1 winners from 2023 to 2016 with circuit, winner and season columns." /></noscript>
    </div>
  </div>
</article>
{% endblock %}